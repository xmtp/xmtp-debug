  "devDependencies": {
    "@types/node": "^18.19.33",
    "@types/yargs": "^17.0.13",
    "eslint": "^9.4.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@xmtp/proto": "3.54.0",
